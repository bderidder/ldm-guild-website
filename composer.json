{
  "name": "La Danse Guild Website",
  "license": "MIT",
  "type": "project",
  "description": "Symfony project for the La Danse Guild Website",
  "autoload": {
    "psr-4": { "": "src/" },
    "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
  },
  "autoload-dev": {
    "psr-4": { "Tests\\": "tests/" }
  },
  "require": {
    "php": "~7.0",
    "symfony/symfony": "3.4.*",
    "doctrine/orm": "~2.5",
    "doctrine/doctrine-bundle": "~1.6",
    "twig/extensions": "~1.0",
    "symfony/assetic-bundle": "~2.8",
    "symfony/swiftmailer-bundle": "~2.3",
    "symfony/monolog-bundle": "~2.8",
    "sensio/distribution-bundle": "~5.0",
    "sensio/framework-extra-bundle": "~3.0.2",
    "sensio/generator-bundle": "~3.0",
    "incenteev/composer-parameter-handler": "~2.0",
    "doctrine/doctrine-migrations-bundle": "~1.0",
    "mopa/bootstrap-bundle": "3.1.2",
    "jms/di-extra-bundle": "~1.5",
    "jms/aop-bundle": "^1.1",
    "jms/serializer-bundle": "^1.1",
    "friendsofsymfony/rest-bundle": "~1.4",
    "craue/formflow-bundle": "~3.0",
    "friendsofsymfony/user-bundle": "~2.0",
    "coderspotting/toast-message": "~1.0",
    "eluceo/ical": "^0.9",
    "yohang/finite": "^1.1",
    "trt/swift-css-inliner-bundle": "^0.4",
    "hwi/oauth-bundle": "~0.5",
    "guzzlehttp/guzzle": "^6.2",
    "sp/bower-bundle": "^0.12@dev",
    "nelmio/api-doc-bundle": "^2.13",
    "friendsofsymfony/jsrouting-bundle": "^1.6"
  },
  "require-dev": {
    "phpunit/phpunit": "@stable",
    "phake/phake": "@stable",
    "liip/functional-test-bundle": "@stable",
    "doctrine/doctrine-fixtures-bundle": "@stable",
    "nelmio/alice": "@stable"
  },
  "scripts": {
    "post-install-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Sp\\BowerBundle\\Composer\\ScriptHandler::bowerInstall"
    ],
    "post-update-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Sp\\BowerBundle\\Composer\\ScriptHandler::bowerInstall"
    ]
  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-web-dir": "web",
    "symfony-var-dir": "var",
    "symfony-bin-dir": "bin",
    "symfony-tests-dir": "tests",
    "symfony-assets-install": "symlink",
    "incenteev-parameters": {
      "file": "app/config/parameters.yml"
    }
  }
}
