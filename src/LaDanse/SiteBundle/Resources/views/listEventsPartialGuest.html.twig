        <table cellpadding=5 border=0>
        {% for event in events %}
            <tr>
                <td class="center-td">{{ event.inviteTime|date("d/m (D)") }}</td>
                <td class="center-td">{{ event.inviteTime|date("H:i") }}</td>
                <td>
                    <a href="{{ path('viewEventIndex', {'id': event.id}) }}">{{ event.name }}</a>
                </td>
            </tr>
        {% endfor %}
        </table>