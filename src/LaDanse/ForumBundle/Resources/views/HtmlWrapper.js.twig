<script>

HtmlWrapper.prototype = new UIComponent();
HtmlWrapper.prototype.constructor = HtmlWrapper;

function HtmlWrapper(wrapperId, wrappedComponent)
{
    UIComponent.prototype.constructor.call(this);

    this.wrapperId = wrapperId;
    this.wrappedComponent = wrappedComponent;
};

HtmlWrapper.prototype.render = function()
{
    var html = this.wrappedComponent.render();

    $(this.wrapperId).html(html);
}

</script>